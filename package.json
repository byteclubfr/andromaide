{
  "name": "andromaide",
  "version": "1.0.0",
  "description": "GUI to play with chained steps (Promises...)",
  "main": "bundle.js",
  "scripts": {
    "build": "mkdir -p dist && npm run components && npm run styles && npm run html",
    "components": "browserify src/scripts/index.js --debug -t [babelify --stage 0] --outfile dist/bundle.js",
    "html": "cp src/index.html dist",
    "start": "http-server dist",
    "styles": "stylus src/styles/style.styl --out dist/",
    "w-styles": "stylus -w src/styles/styles.styl --out dist/"
  },
  "author": "Delapouite <delapouite@gmail.com> (http://delapouite.com)",
  "license": "ISC",
  "dependencies": {
    "react": "^0.13.3",
    "redux": "^0.12.0"
  },
  "devDependencies": {
    "babelify": "^6.1.3",
    "browserify": "^11.0.1",
    "eslint": "^1.0.0",
    "eslint-plugin-react": "^3.2.0",
    "stylus": "^0.52.0"
  }
}
